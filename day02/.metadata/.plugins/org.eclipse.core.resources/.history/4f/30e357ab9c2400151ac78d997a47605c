package com.geminno.continuebreakdemo;

public class Demo3 {

	/**
	 * @param args
	 */
	//1-100之间质数的个数
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int num=0;
		for(int i=2;i<=100;i++){
			boolean flag=true;
			for(int j=2;j<i;j++){
				if(i%j==0){
					flag=false;
					break;
				}
			}
		}
		num++;
		System.out.println("1-100之间质数的个数是："+num);
	}

}
