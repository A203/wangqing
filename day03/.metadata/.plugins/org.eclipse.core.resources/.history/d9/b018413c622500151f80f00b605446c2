package com.geminno.exercise;

public class Modifier01 {
    
	//公共访问修饰符修饰的方法
	public void methodM1(){
		System.out.println("public methodM1");
	}
	
	//私有方法
	private void methodM2(){
		System.out.println("private methodM2");
	}
	
	//default范围的方法（同一个也可以访问）
	void methodM3(){
		System.out.println("default methodM3");
	}
	
	//protect(同一个包或者子类也可以访问)
	protected void methodM4(){
		System.out.println("protected methodM4");
	}
	
}


