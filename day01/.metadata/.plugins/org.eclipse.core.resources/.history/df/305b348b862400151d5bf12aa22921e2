package com.geminno.demo;

public class Demo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int i=5;
        int j=3;
//        System.out.println("i+j="+(i+j));
//        System.out.println("i-j="+(i-j));
//        System.out.println("i/j="+(i/j));
//        System.out.println("i%j="+(i%j));
        
        //前缀递增
        System.out.println(++i);
        //后缀递减
        System.out.println(j--);
        
        //关系运算符
        if(i>=j)
        {
        	System.out.println("i大于等于j");
        }
	}

}
