	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
     boolean studyHard=true;
     if(studyHard)
     {
    	 System.out.println("考试能通过");
     }	
     else
     {
    	 System.out.println("考试不能通过");
     }
	}

}
