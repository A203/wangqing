package com.geminno.exercisedemo;

public class Exe03 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//byte类型的取值
		byte a1=5;
	    // int类型的取值
		int a2=3;
		//double类型的取值
		double a3=23.37;
		//float类型的取值
	    float a4=10.0f;
	    //char类型的取值
	    char a5='a';
	    //boolean类型的取值
	    boolean a6=true;
	    System.out.println(a1+"  "+a2+"  "+a3);
	    System.out.println(a4+"  "+a5+"  "+a6);
	}

}
