package com.geminno.demo;

public class VarDemo {

	/**
	 * @param args
	 */
	static int number;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//
        System.out.println(VarDemo.number);
        //访问方式，对象，属性名
        VarDemo demo1=new VarDemo();
        System.out.println(demo1.number);
	}

}
